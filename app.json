{
    "expo": {
        "name": "Shopkeeper Sales Tracker",
        "slug": "shopkeeper-sales-tracker",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "rork-app",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/images/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#F57C00"
        },
        "ios": {
            "supportsTablet": false,
            "bundleIdentifier": "app.rork.shopkeeper-sales-tracker"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#F57C00"
            },
            "package": "app.rork.shopkeeper_sales_tracker",
            "permissions": [
                "android.permission.VIBRATE"
            ]
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            [
                "expo-router",
                {
                    "origin": "https://rork.com/"
                }
            ],
            "expo-font",
            "expo-web-browser"
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}